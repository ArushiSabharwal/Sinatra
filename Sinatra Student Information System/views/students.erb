<h1> Student List </h1>
<h3 style="color:red"><%= @msg %></h3>
<table class="table">
  <thead>
    <tr>
      <th>Student ID</th>
      <th>Student Name</th>
    </tr>
  </thead>
  <tbody>
     <% @students.each do |student| %>
      <tr>
        <td><a href="/students/<%= student.student_id %>"><%= student.student_id %></a></td>
        <td><a href="/students/<%= student.student_id %>"><%= student.firstname + " " + student.lastname %></a></td>
      </tr>
     <% end %>
  </tbody>
</table>
<a class="btn btn-primary" href="/newstu" role="button"> Add New Student </a>
